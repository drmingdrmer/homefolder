fun! edit#digraph#trigger() "{{{
    " remove 2 char before cursor.
    " and use them to trigger a digraph.
    " See: :help digraph
    let [x, l, c, y] = getpos(".")
    let line = getline(l)
    let upto_col = line[:c-2]
    let c1c2 = matchstr(upto_col, '\v..$')
    if c1c2 == ''
        return ""
    endif
    if has_key(s:digraphs, c1c2)
        return "\<BS>\<BS>" . s:digraphs[c1c2][0]
    endif
    return ""
endfunction "}}}


